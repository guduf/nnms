#!/usr/bin/env node
const NNMS_VERSION = process.env['NNMS_VERSION']
if (!NNMS_VERSION) {
  console.error(`ERROR missing env 'NNMS_VERSION'`)
  process.exit(1)
}
console.log(`nnms-start v${NNMS_VERSION}`)
let config
try { config = require() } catch (err) {
  console.error(err)
  console.error(`ERROR cannot require './nnms.json'`)
}
